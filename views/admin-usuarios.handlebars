<div class="container">
	<div class="row">
		<div class="col-9 offset-1">
			<h1>Bienvenido Admin</h1>
			<a href="/">‚Üê Back To Home</a>
			<div class="row">

				<table class="table table-dark">
					<thead>
						<tr>
							<th scope="col">Id</th>
							<th scope="col">Nombre</th>
							<th scope="col">Correo</th>
							<th scope="col">Telefono</th>
						</tr>
					</thead>

					{{#each datos}}

					<tr class="col">
						<td>{{id}}</td>
						<td>{{name}}</td>
						<td>{{correo}}</td>
						<td>{{telefono}}</td>

					</tr>
					{{/each}}

				</table>

				<form>
					<div class="row">
						<div class="col">
							<input id="nombre" type="text" class="form-control" placeholder="Nombre">
						</div>
						<div class="col">
							<input id="correo" type="text" class="form-control" placeholder="Correo">
						</div>
						<div class="col">
							<input id="telefono" type="text" class="form-control" placeholder="Telefono">
						</div>
						<div class="col">
							<button id="submit" type="submit" class="btn btn-success">Agregar</button>
						</div>
					</div>
				</form>

			</div>
		</div>
	</div>
</div>

<script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
<script src="/js/admin-usuarios.js"></script>